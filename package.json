{
  "name": "@hedgedoc/markdown-it-task-lists",
  "version": "1.0.3",
  "description": "A markdown-it plugin to create GitHub-style task lists",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSES/*",
    "package.json",
    "README.md",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublish": "tsc",
    "lint": "eslint --max-warnings=0 --ext .ts src",
    "format": "prettier -c \"{src,test}/**/*.{ts,tsx,js}\"",
    "format:fix": "prettier -w \"{src,test}/**/*.{ts,tsx,js}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^28.0.0",
    "@types/jsdom": "^16.2.13",
    "@types/markdown-it": "^12.2.1",
    "@types/punycode": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^28.0.0",
    "jsdom": "^19.0.0",
    "markdown-it": "^13.0.0",
    "prettier": "2.7.1",
    "punycode": "^2.1.1",
    "ts-jest": "^28.0.0",
    "typescript": "^4.4.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedgedoc/markdown-it-better-task-lists.git"
  },
  "bugs": {
    "url": "https://github.com/hedgedoc/markdown-it-better-task-lists/issues"
  },
  "homepage": "https://github.com/hedgedoc/markdown-it-better-task-lists#readme",
  "packageManager": "yarn@3.2.1"
}
